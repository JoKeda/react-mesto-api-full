{
  "extends": "airbnb-base",
  "ignorePatterns": ["/node_modules/*"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": ["error", { "allow": ["warn", "error","log"] }],
    "max-classes-per-file": ["error",10],
    "consistent-return": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}